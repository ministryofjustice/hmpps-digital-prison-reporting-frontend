{% from "govuk/components/table/macro.njk" import govukTable %}
{% from "../search-copy/view.njk" import dprSearchCopy %}

{% macro dprReportsList(head, rows) %}

  {% set tableHtml %}
  {{ govukTable({
    captionClasses: "govuk-table__caption--m",
    head: head,
    rows: rows,
    classes: "dpr-search-table"
  }) }}
  {% endset %}

  {{ dprSearchCopy(tableHtml) }}

{% endmacro %}
