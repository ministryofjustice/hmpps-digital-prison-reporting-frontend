{% extends layoutPath %}
{% from "../components/async-filters/view.njk" import dprAsyncFilters %}

{% set mainClasses = "app-container govuk-body" %}
{% block content %}
  <h1>{{ title }}</h1>
  <p class="govuk-body">{{ description }}</p>
    
  {{ dprAsyncFilters({ 
    filters: filters, 
    sortBy: sortBy, 
    data: reportData, 
    endpoint: postEndpoint,
    type: type 
  }) }}
{% endblock %}
