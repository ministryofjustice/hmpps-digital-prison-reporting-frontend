{% from "../components/chart-card/view.njk" import dprChartCard %}

{% extends layoutPath %}
{% set mainClasses = "app-container govuk-body" %}
{% block content %}
  <h1>{{ title }}</h1>
  <p class="govuk-body">{{ description }}</p>
  
  {% for chart in data %}
    <div class="govuk-!-margin-bottom-6 govuk-!-padding-bottom-6">
      {{ dprChartCard(chart) }}
    </div>
  {% endfor %}


{% endblock %}
